	<queryString>
		<![CDATA[select v.data, v.hora,
	case v.tipoPagamento
			when "VP" then "Venda à Prazo"
			when "VV" then "Venda à Vista"
			when "VC" then "Venda à Cartão"
	end as tipoPagamento,
	case v.cancelada
		when true then "Cancelada"
		when false then ""
	end as cancelada
		,
	v.valortotal,
	case v.cancelada
		when true then 0
		when false then v.vlCartao
	end as vlCartao,
	case v.cancelada
		when true then 0
		when false then v.vlPromissoria
	end as vlPromissoria,
	case v.cancelada
		when true then 0
		when false then v.vlVista
	end as vlVista, c.nome as nomeCLiente,
	ve.nome as nomeVendedor,v.desconto,
 v.valortotal-v.desconto as
	valorliquido from venda v
inner join cliente c on c.id=v.cliente_id
inner join vendedor ve on ve.id=v.vendedor_id
where v.data=$P{data}
order by c.nome]]>
	</queryString>
