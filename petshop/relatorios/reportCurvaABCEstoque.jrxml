	<parameter name="dtInicial" class="java.lang.String"/>
	<parameter name="dtFinal" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
	p.codigo,
	p.descricao,
	iv.valorUni,
	sum(iv.quantidade) quant,
	iv.valorUni*sum(iv.quantidade) quantXvalor,
	((iv.valorUni*sum(iv.quantidade))*100)/

	(select sum(quantidade*valorUni) from itemvenda inner join produto on produto.id=itemvenda.produto_id and produto.servico=false
			inner join venda on venda.id=itemvenda.venda_id and venda.data between $P!{dtInicial} and $P!{dtFinal}) porc

	from itemvenda iv
inner join produto p on p.id=iv.produto_id
inner join venda v on v.id=iv.venda_id
where p.servico=false and v.data between $P!{dtInicial} and $P!{dtFinal}
group by p.id
order by 4 desc;
]]>
	</queryString>
